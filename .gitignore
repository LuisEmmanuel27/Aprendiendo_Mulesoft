# .gitignore

# Ignore everything in the root except the "src" and ".gitignore" files.
/*
!/src
!/.gitignore

# Ignore everything in the "src" directory, except the "main" and "test" directories.
/src/*
!/src/main
!/src/test

# Ignore everything in the "main" directory, except the "java" and "resources" directories.
/src/main/*
!/src/main/java
!/src/main/resources

# Ignore everything in the "test" directory, except the "java" and "resources" directories.
/src/test/*
!/src/test/java
!/src/test/resources

# Ignore all files that end with ".class".
**/*.class

# Ignore all log files.
**/*.log

# Ignore the project files generated by Anypoint Studio.
/.project
/.classpath
/.settings/

# Ignore the files generated by Mule.
/test-classes/
/target/
/logs/
/console/
/mule-enterprise-standalone-*/
/domains/
/apps/